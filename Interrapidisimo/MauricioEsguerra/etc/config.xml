<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<carriers>
			<intershipping>
				<model>Interrapidisimo\MauricioEsguerra\Model\Carrier\Intershipping</model>
				<active>0</active>
				<title>Intershipping</title>
				<name>Intershipping</name>
				<price>0.00</price>
				<specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
				<sallowspecific>0</sallowspecific>
			</intershipping>
		</carriers>
		<payment>
            <mercadopago_custom>
                <active>0</active>
                <model>Interrapidisimo\MauricioEsguerra\Model\Payment\MercadoPago\Custom</model>
                <payment_action>authorize_capture</payment_action>
                <title>Mercado Pago - Tarjeta de Crédito/Débito</title>
                <can_authorize>1</can_authorize>
                <can_capture>1</can_capture>
                <can_void>1</can_void>
                <can_refund>1</can_refund>
                <can_use_checkout>1</can_use_checkout>
                <is_gateway>1</is_gateway>
                <sort_order>10</sort_order>
                <public_key></public_key>
                <access_token></access_token>
                <country_code>CO</country_code>
                <debug_mode>0</debug_mode>
                <allowspecific>0</allowspecific>
                <specificcountry></specificcountry>
                <min_order_total></min_order_total>
                <max_order_total></max_order_total>
            </mercadopago_custom>
        </payment>
	</default>
</config>
